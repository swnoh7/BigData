package 변수및자료형;

import java.util.Scanner;

public class Ex1227 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("글자를 입력해주세요 :");
		sc.next();
		
	    char name = 'J';
	    String name2 = "James";
	    
	    System.out.println(name);
	    System.out.println(name2);
	    
	    boolean real = true;
	    real = false;
	    System.out.println(real);
	    
		
		
		
		
		
		
		
		
		
		
		
	}

}
